; GR3 := Klocka
; $ee := Klocka framtid
; GR2 := VGA_IN
; GR1 := VGA_OUT
;$ff - siffra 6
;$fe - siffra 5
;$fd - siffra 4
;$fc - siffra 3
;$fb - siffra 2
;$fa - siffra 1
        ADD     GR0,#10
        STORE   GR0,$ee
MAIN    MOVE    0
        JMP     SEND
TIME    CMP     GR3,$ee
        BGE     SCORE
        JMP     MAIN

SCORE   LOAD    GR0,$ee
        ADD     GR0,#10
        STORE   GR0,$ee
        LOAD    GR0,$fa
        ADD     GR0,#1
        CMP     GR0,#10
        BGE     ONE
        JMP     MAIN
ONE_R   LOAD    GR0,$fb
        ADD     GR0,#1
        CMP     GR0,#10
        BGE     TEN
        JMP     MAIN
TEN_R   LOAD    GR0,$fc
        ADD     GR0,#1
        CMP     GR0,#10
        BGE     HUN
        JMP     MAIN
HUN_R   LOAD    GR0,$fd
        ADD     GR0,#1
        CMP     GR0,#10
        BGE     THD
        JMP     MAIN
THD_R   LOAD    GR0,$fe
        ADD     GR0,#1
        CMP     GR0,#10
        BGE     TTT
        JMP     MAIN
TTT_R   LOAD    GR0,$ff
        ADD     GR0,#1
        CMP     GR0,#10
        BGE     TNT
        JMP     MAIN

ONE     LOAD    GR0,#0
        STORE   GR0,$fa
        JMP     ONE_R
TEN     LOAD    GR0,#0
        STORE   GR0,$fb
        JMP     TEN_R
HUN     LOAD    GR0,#0
        STORE   GR0,$fc
        JMP     TEN_R
THD     LOAD    GR0,#0
        STORE   GR0,$fd
        JMP     THD_R
TTT     LOAD    GR0,#0
        STORE   GR0,$fe
        JMP     TTT_R
TNT     LOAD    GR0,#0
        STORE   GR0,$ff
        JMP     MAIN

SEND    CMP     GR2,#6
        BGE     SIXTH
        CMP     GR2,#5
        BGE     SIXTH
        CMP     GR2,#4
        BGE     SIXTH
        CMP     GR2,#3
        BGE     SIXTH
        CMP     GR2,#2
        BGE     SIXTH
        CMP     GR2,#1
        BGE     FIRST
        JMP     TIME

FIRST   STORE   GR1,$fa
        JMP     TIME
SECOND  STORE   GR1,$fb
        JMP     TIME
THIRD   STORE   GR1,$fc
        JMP     TIME
FOURTH  STORE   GR1,$fd
        JMP     TIME
FIFTH   STORE   GR1,$fe
        JMP     TIME
SIXTH   STORE   GR1,$ff
        JMP     TIME
